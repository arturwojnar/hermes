{
  "name": "ts-outbox",
  "version": "0.0.0-development.0",
  "description": "Reliable implementation of outbox pattern in TypeScript",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "cache": "~/.npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturwojnar/ts-outbox.git"
  },
